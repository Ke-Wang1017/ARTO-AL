      <mass value="1"/>
      <origin rpy="0 1.57 0" xyz="0 0 0"/>
      <inertia ixx="0.0041" ixy="0.0" ixz="0.0" iyy="0.0041" iyz="0.0" izz="0.0008"/>
    </inertial>
  </link>

