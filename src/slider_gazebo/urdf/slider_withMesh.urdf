    </collision> -->
    <inertial>
      <mass value="8.5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
      <inertia ixx="0.0275" ixy="0" ixz="0" iyy="0.0708" iyz="0" izz="0.0524"/>
    </inertial>
  </link>

